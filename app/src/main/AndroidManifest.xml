<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.swornim.musicnap">

    <uses-feature
        android:name="android.hardware.camera2"
        android:required="true" />

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icon_app_1x"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".customAdapterPackage.test"
            android:launchMode="singleTask"
            android:windowSoftInputMode="adjustPan">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".customAdapterPackage.MediaPlayerClass" />
        <activity android:name=".customAdapterPackage.UploadFileFirebase" />
        <activity
            android:name=".customAdapterPackage.StreamTest"
            android:windowSoftInputMode="adjustPan" />
        <activity android:name=".customAdapterPackage.friendlist" />

        <service android:name=".customAdapterPackage.FirebaseBackgroundService" />

        <activity android:name=".MainActivity" />
        <activity
            android:name=".customAdapterPackage.SnapMusicHomePage"
            android:windowSoftInputMode="adjustPan" />
        <activity android:name=".Main2Activity" />
        <activity android:name=".customAdapterPackage.storytest"></activity>
    </application>

</manifest>